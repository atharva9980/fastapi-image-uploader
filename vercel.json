{
  "version": 2,
  "builds": [
    {
      "src": "api/upload_service.py",
      "use": "@vercel/fastapi"

    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/upload_service.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/uploader.html"
    }
  ]
}